<struts>
 <package name="test" namespace="/" extends="struts-default">
   <global-results>
     <result name="list" type="redirectAction">list</result>
   </global-results>
   
   <action name="list" class="com.woniu.action.DeptAction" method="list">
	  <result name="show">/show.jsp</result>
   </action>
   
   <action name="save" class="com.woniu.action.DeptAction" method="save"></action>
   <action name="delete" class="com.woniu.action.DeptAction" method="delete"></action>
   <action name="update" class="com.woniu.action.DeptAction" method="update"></action>
   
   <action name="findOne" class="com.woniu.action.DeptAction" method="findOne">
	  <result name="updateForm">/updateForm.jsp</result>
   </action>

 </package>
</struts>